#include<iostream>
using namespace std;
int main (){
    int n,m,a;
    a=0;
    cin>>n>>m;
 
 for(int i=n+1;i<=m;i++){
        for(int j=1;j<=i;j++){
 
 
            if(i%j==0){
                a++;
            }
        }
        if(a<=2){
                if(i==m){
                    cout<<"YES"<<endl;
                }
                else{
                    cout<<"NO"<<endl;
                }
                break;
 
        }
        else if(i==m &&a>2){
            cout<<"NO"<<endl;
        }
 
    a=0;
 
 
 
 
 
 }
 
 
 
return 0;
 
}
