#include <iostream>
using namespace std;
int main()
{
    int n, h, a;
    a = 0;
    cin >> n >> h;
    int array[n];
    for (int i = 0; i < n; i++)
    {
        cin >> array[i];
    }
    for (int i = 0; i < n; i++)
    {
        if (array[i] > h)
        {
            a = a + 2;
        }
        else
        {
            a += 1;
        }
    }
    cout << a << endl;
 
    return 0;
}
